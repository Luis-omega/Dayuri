
SPACE : " "|"\t"|"\n"

%ignore SPACE


IDENTIFIER : /(?=\D)\w+/u

UINT : /([1-9][0-9_]*)|(0(0|_)*)/

ARROW.6 : "->"

BASE_TYPES.10 : "int"

type_atom: BASE_TYPES
  | "(" type_expression ")"

type_arrow: type_atom (ARROW type_atom)*

type_expression : type_arrow 





test_type_expression : (type_expression ";")+
